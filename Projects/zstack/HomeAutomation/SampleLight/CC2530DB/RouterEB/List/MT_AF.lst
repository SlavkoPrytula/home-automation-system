    272          static void MT_AfRegister(uint8 *pBuf)
    273          {
    274            uint8 cmdId;
    275            uint8 retValue = ZMemError;
    276            endPointDesc_t *epDesc;
    277          
    278            /* parse header */
    279            cmdId = pBuf[MT_RPC_POS_CMD1];
    280            pBuf += MT_RPC_FRAME_HDR_SZ;
    281          
