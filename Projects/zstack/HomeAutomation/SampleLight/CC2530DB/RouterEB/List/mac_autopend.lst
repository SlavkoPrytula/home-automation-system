    359                  * 2n bit and 2n+1 bit of shortAddrEnable must be clear in order
    360                  * to assume that the entry location for an extended address
    361                  * is not used.        
    362                  */
    363                 if( (enable & ((uint24)0x03 << (index*2))) == 0 )
    364                 {
    365                   return index;
    366                 }
    367               }
    368             }
    369            return index;
    370          }
