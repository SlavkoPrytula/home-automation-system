   1609            {
   1610              count = 0;
   1611            }
   1612            else
   1613            {
   1614              count = ResultCount - zdappMgmtNwkDiscStartIndex;
   1615              if ( count > ZDO_MAX_NWKDISC_ITEMS )
   1616              {
   1617                count = ZDO_MAX_NWKDISC_ITEMS;
   1618              }
