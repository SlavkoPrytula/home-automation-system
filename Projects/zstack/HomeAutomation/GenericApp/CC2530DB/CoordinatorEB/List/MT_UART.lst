    308          /***************************************************************************************************
    309           * @fn      MT_UartProcessZAppData
    310           *
    311           * @brief   | SOP | CMD  |   Data Length   | FSC  |
    312           *          |  1  |  2   |       1         |  1   |
    313           *
    314           *          Parses the data and determine either is SPI or just simply serial data
    315           *          then send the data to correct place (MT or APP)
    316           *
    317           * @param   port    - UART port
    318           *          event   - Event that causes the callback
    319           *
    320           *
    321           * @return  None
    322           ***************************************************************************************************/
    323          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    324          {
    325          
    326            osal_event_hdr_t  *msg_ptr;
    327            uint16 length = 0;
    328            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    329          
    330            // NEW
    331            osal_set_event(8, 0x0002);
    332            return;
    333            // ENDNEW
    334            
    335            /*
    336               If maxZAppBufferLength is 0 or larger than current length
    337               the entire length of the current buffer is returned.
    338            */
    339            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    340            {
    341              length = MT_UartMaxZAppBufLen;
    342            }
    343            else
    344            {
    345              length = rxBufLen;
    346            }
    347          
    348            /* Verify events */
    349            if (event == HAL_UART_TX_FULL)
    350            {
    351              // Do something when TX if full
    352              return;
    353            }
    354          
    355            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    356            {
    357              if ( App_TaskID )
    358              {
    359                /*
    360                   If Application is ready to receive and there is something
    361                   in the Rx buffer then send it up
    362                */
    363                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    364                {
    365                  /* Disable App flow control until it processes the current data */
    366                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    367          
    368                  /* 2 more bytes are added, 1 for CMD type, other for length */
    369                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    370                  if ( msg_ptr )
    371                  {
    372                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    373                    msg_ptr->status = length;
    374          
    375                    /* Read the data of Rx buffer */
    376                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    377          
    378                    /* Send the raw data to application...or where ever */
    379                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    380                  }
    381                }
    382              }
    383            }
