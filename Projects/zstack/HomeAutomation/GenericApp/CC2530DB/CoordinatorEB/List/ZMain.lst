    299           * @brief       This displays the IEEE (MSB to LSB) on the LCD.
    300           *
    301           * input parameters
    302           *
    303           * None.
    304           *
    305           * output parameters
    306           *
    307           * None.
    308           *
    309           * @return      None.
    310           **************************************************************************************************
    311           */
    312          static void zmain_dev_info(void)
    313          {
    314          #if defined ( SERIAL_DEBUG_SUPPORTED ) || (defined ( LEGACY_LCD_DEBUG ) && defined (LCD_SUPPORTED))
    315            uint8 i;
    316            uint8 *xad;
    317            uint8 lcd_buf[Z_EXTADDR_LEN*2+1];
    318          
    319            // Display the extended address.
    320            xad = aExtendedAddress + Z_EXTADDR_LEN - 1;
    321          
    322            for (i = 0; i < Z_EXTADDR_LEN*2; xad--)
    323            {
    324              uint8 ch;
    325              ch = (*xad >> 4) & 0x0F;
    326              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    327              ch = *xad & 0x0F;
    328              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    329            }
    330            lcd_buf[Z_EXTADDR_LEN*2] = '\0';
    331            HalLcdWriteString( "IEEE: ", HAL_LCD_DEBUG_LINE_1 );
    332            HalLcdWriteString( (char*)lcd_buf, HAL_LCD_DEBUG_LINE_2 );
    333          #endif
    334          }
